<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="consoleService" name="Arduino Launch Console Service" schema="schema/consoleService.exsd"/>
   <extension
         point="org.eclipse.cdt.managedbuilder.core.buildDefinitions">
      <managedBuildRevision
            fileVersion="4.0.0">
      </managedBuildRevision>
      <toolChain
            archList="all"
            configurationEnvironmentSupplier="ca.cdtdoug.wascana.arduino.core.EnvVarSupplier"
            id="ca.cdtdoug.wascana.arduino.toolChain.avr"
            isAbstract="false"
            languageSettingsProviders="ca.cdtdoug.wascana.arduino.avr.builtins;ca.cdtdoug.wascana.arduino.avr.outputParser"
            name="Arduino AVR C++"
            osList="all"
            supportsManagedBuild="false">
         <targetPlatform
               id="ca.cdtdoug.wascana.arduino.targetPlatform"
               isAbstract="false"
               name="Arduino">
         </targetPlatform>
         <builder
               autoBuildTarget="all"
               buildRunner="org.eclipse.cdt.managedbuilder.core.ExternalBuildRunner"
               cleanBuildTarget="clean"
               command="make"
               commandLauncher="org.eclipse.cdt.core.CommandLauncher"
               id="ca.cdtdoug.wascana.arduino.builder.make"
               incrementalBuildTarget="all"
               isAbstract="false"
               isVariableCaseSensitive="false"
               name="Arduino Make">
         </builder>
         <tool
               command="avr-g++"
               id="ca.cdtdoug.wascana.arduino.tool.avr.compiler"
               isAbstract="false"
               name="Arduino AVR C++ Compiler"
               natureFilter="ccnature"
               superClass="cdt.managedbuild.tool.gnu.cpp.compiler.base">
         </tool>
         <option
               category="ca.cdtdoug.wascana.arduino.core.optionCategory.board"
               id="ca.cdtdoug.wascana.arduino.core.option.board"
               isAbstract="false"
               name="Board Type"
               resourceFilter="all"
               valueType="string">
         </option>
         <optionCategory
               id="ca.cdtdoug.wascana.arduino.core.optionCategory.board"
               name="Board Type">
         </optionCategory>
      </toolChain>
   </extension>
   <extension
         point="org.eclipse.cdt.core.LanguageSettingsProvider">
      <provider
            class="ca.cdtdoug.wascana.arduino.core.AvrLanguageSettingsProvider"
            id="ca.cdtdoug.wascana.arduino.avr.builtins"
            name="Arduino AVR Compiler Built-ins"
            parameter="${COMMAND} ${FLAGS} -E -P -v -dD &quot;${INPUTS}&quot;"
            prefer-non-shared="true">
         <language-scope
               id="org.eclipse.cdt.core.g++">
         </language-scope>
      </provider>
      <provider
            class="org.eclipse.cdt.managedbuilder.language.settings.providers.GCCBuildCommandParser"
            id="ca.cdtdoug.wascana.arduino.avr.outputParser"
            name="Arduino AVR Built Output Parser"
            parameter="(avr-gcc)|(avr-g\+\+)"
            prefer-non-shared="true">
      </provider>
   </extension>
   <extension
         point="org.eclipse.launchbar.core.launchBarContributions">
      <targetType
            class="ca.cdtdoug.wascana.arduino.core.launch.ArduinoLaunchTargetType"
            id="ca.cdtdoug.wascana.arduino.core.targetType">
      </targetType>
      <descriptorType
            class="ca.cdtdoug.wascana.arduino.core.launch.ArduinoLaunchDescriptorType"
            id="ca.cdtdoug.wascana.arduino.core.descriptorType"
            priority="5">
      </descriptorType>
      <configProvider
            class="ca.cdtdoug.wascana.arduino.core.launch.ArduinoLaunchConfigurationProvider"
            launchConfigurationType="ca.cdtdoug.wascana.arduino.core.launchConfigurationType">
      </configProvider>
      <configType
            descriptorType="ca.cdtdoug.wascana.arduino.core.descriptorType"
            isDefault="true"
            launchConfigurationType="ca.cdtdoug.wascana.arduino.core.launchConfigurationType"
            targetType="ca.cdtdoug.wascana.arduino.core.targetType">
      </configType>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="ca.cdtdoug.wascana.arduino.core.launch.ArduinoLaunchConfigurationDelegate"
            id="ca.cdtdoug.wascana.arduino.core.launchConfigurationType"
            modes="run"
            name="Arduino">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.core.contenttype.contentTypes">
      <content-type
            base-type="org.eclipse.cdt.core.binaryFile"
            file-extensions="hex"
            id="ca.cdtdoug.wascana.arduino.core.contentType.hex"
            name="Arduino Hex download file"
            priority="normal">
      </content-type>
   </extension>
   <extension
         id="arduinoNature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="ca.cdtdoug.wascana.arduino.core.ArduinoProjectNature">
         </run>
      </runtime>
   </extension>
</plugin>
